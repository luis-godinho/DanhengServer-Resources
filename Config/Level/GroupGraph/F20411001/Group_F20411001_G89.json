{
  "OnInitSequece": [
    {
      "TaskList": []
    }
  ],
  "OnStartSequece": [
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.LoopWaitServerFloorSavedValueChange"
        }
      ]
    },
    {
      "TaskList": [
        {
          "$type": "RPG.GameCore.PredicateTaskList",
          "Predicate": {
            "$type": "RPG.GameCore.ByCompareFloorSavedValue",
            "DynamicName": {
              "Value": "FSV_Teleport"
            },
            "CompareType": "Equal",
            "CompareValue": 1
          },
          "SuccessTaskList": [
            {
              "$type": "RPG.GameCore.ChangePropState"
            }
          ],
          "FailedTaskList": [
            {
              "$type": "RPG.GameCore.PredicateTaskList",
              "Predicate": {
                "$type": "RPG.GameCore.ByCompareFloorSavedValue",
                "DynamicName": {
                  "Value": "FSV_Teleport"
                },
                "CompareType": "Equal"
              },
              "SuccessTaskList": [
                {
                  "$type": "RPG.GameCore.ChangePropState"
                }
              ]
            }
          ]
        }
      ]
    }
  ],
  "Type": "Group",
  "EntityEventList": []
}